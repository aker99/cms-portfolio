{
  "kind": "collectionType",
  "collectionName": "experiences",
  "info": {
    "name": "Experience"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "profile": {
      "type": "string",
      "required": true
    },
    "organization": {
      "type": "string",
      "required": true
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date"
    },
    "desc": {
      "type": "richtext"
    },
    "location": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Volunteer",
        "Internship",
        "PartTime",
        "FullTime",
        "Remote",
        "Freelance"
      ],
      "unique": false,
      "required": false,
      "default": "FullTime"
    }
  }
}
